{
  "name": "propertypro",
  "version": "1.0.0",
  "description": "PropertyPro Lite",
  "main": "index.js",
  "scripts": {
    "build": "babel ./API/src -d ./API/lib",
    "dev": "nodemon --exec babel-node ./API/src/index.js",
    "start": "node ./API/lib/index.js",
    "start-db": "babel-node ./API/src/db/migrations.js",
    "test": "mocha ./API/lib/test/*.test.js",
    "test-chk": "npm run start-db && npm run build && mocha ./API/lib/test/*.test.js --timeout 30000 --exit",
    "test-watch": "nodemon --exec  \"npm test\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okikiola11/PropertyPro.git"
  },
  "keywords": [
    "property",
    "apartment",
    "users",
    "user",
    "agent",
    "lands",
    "houses"
  ],
  "author": "Apelehin Okikiola",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/okikiola11/PropertyPro/issues"
  },
  "homepage": "https://github.com/okikiola11/PropertyPro#readme",
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.14.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.1.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.1",
    "multer-storage-cloudinary": "^2.2.1",
    "pg": "^7.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.2",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "mocha": "^6.1.4",
    "prettier": "^1.18.2",
